<li data-idZone="{{ zoneId }}"
    data-idItem="{{ libre.id }}"
    class="courseElemAdmin freeAdmin col-md-12"
    data-typeItem="libre"
    {% if inSection == 'true' %}
        {% set isVisible = "false" %}
        {% if  visible == 1 %}
            {% set isVisible = "true" %}
        {% endif %}
        data-IsVisible="{{ isVisible }}"
    {% endif %}
>

    <div class="titleBar">Zone libre</div>
    {% if inSection == 'true' %}
        {% include "cours/adminBtns.html.twig" with {visible: visible, isReferent: isReferent}  %}
    {% else %}
        <div class="btnActionCol col-md-1">
            <ul>
                <li class="myBtn btnDelete btnLeft btnSupprElem"><i class="fa fa-times"></i></li>
            </ul>
        </div>
    {% endif %}

    <div class="formCol col-md-11">
        <div class="row">
            <div class="col-xs-12 noPadding">
                <textarea data-champName="description" id="zone_{{ zoneId }}_lien_{{ libre.id }}_descr" class="complexEditor editor champAdmin" readonly>{{ libre.description }}</textarea>
            </div>
        </div>
    </div>
</li>