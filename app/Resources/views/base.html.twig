<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    {% block title %}
        <title>
        {% if template == "studit" %}
            STUDIT
        {% elseif template == "afadec" %}
            AFADEC
        {% endif %}
        </title>
    {% endblock %}

    {% block mystylesheets %}
        <link rel="stylesheet" href="{{ asset('vendors/bootstrap/dist/css/bootstrap.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('vendors/bootstrap-select/dist/css/bootstrap-select.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('vendors/components-font-awesome/css/font-awesome.min.css') }}" />
        <link rel="stylesheet" href="{{ asset('vendors/blueimp-file-upload/css/jquery.fileupload.css') }}" />
        <link rel="stylesheet" href="{{ asset('vendors/blueimp-file-upload/css/jquery.fileupload.css') }}" />

        <!--EDUComp Stylesheet -->
        <link rel="stylesheet" href="{{ asset('css/vendors.css') }}" />
        <link rel="stylesheet" href="{{ asset('css/style.css') }}" />
        <link rel="stylesheet" href="{{ asset('css/components.css') }}" />
        <link rel="stylesheet" href="{{ asset('css/stdi_style.css') }}">
        {% if template == "studit" %}
            <link rel="stylesheet" href="{{ asset('css/studit.css') }}">
        {% endif %}

        <!--Google Fonts-->
        <link href='https://fonts.googleapis.com/css?family=Oswald:300,700,400' rel='stylesheet' type='text/css'>
        <link href='https://fonts.googleapis.com/css?family=Libre+Baskerville:400,400italic,700' rel='stylesheet' type='text/css'>
        <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

        <!-- Fullcalendar Stylesheet-->
        <link rel='stylesheet' href='{{ asset("vendors/fullcalendar/dist/fullcalendar.css") }}' />
    {% endblock %}
    {% if template == "studit" %}
        <link rel="icon" type="image/x-icon" href="{{ asset('faviconSTUDIT.ico?v=4') }}" />
    {% elseif template == "afadec" %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico?v=4') }}" />
    {% endif %}

    {% block javascripts %}
        <script src="{{ asset('vendors/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('vendors/bootstrap/dist/js/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('vendors/bootstrap-select/dist/js/bootstrap-select.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('vendors/blueimp-file-upload/js/vendor/jquery.ui.widget.js') }}" type="text/javascript"></script>
        <script src="{{ asset('vendors/blueimp-file-upload/js/jquery.fileupload.js') }}" type="text/javascript"></script>
        <script src="{{ asset('vendors/blueimp-file-upload/js/jquery.iframe-transport.js') }}" type="text/javascript"></script>

        <!--Educomp Custom Script-->
        <script src="{{ asset('educomp/js/eduCompMain.js') }}" type=text/javascript></script>

        <!-- Respond.js and HTML shiv provide HTML5 support in older browsers like IE9 and 8 -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->

        <!-- Fullcalendar Script-->
        <script src='{{ asset("vendors/moment/min/moment.min.js") }}'></script>
        <script src='{{ asset("vendors/fullcalendar/dist/fullcalendar.js") }}'></script>
        <script src='{{ asset("vendors/fullcalendar/dist/locale/fr.js") }}'></script>

        <! -- bootstrap-number-input -->
        <script src='{{ asset("js/bootstrap-number-input.js") }}'></script>

        <script>
            var monthNames = ["Janvier", "Février", "Mars", "Avril", "Mai", "Juin",
                "Juillet", "Août", "Septembre", "Octobre", "Novembre", "Décembre"
            ];
            jQuery(document).ready(function () {
                $.ajax({
                    type: 'POST',
                    url: "{{path('getNbMsg_ajax')}}",
                    async: false,
                    dataType: "json",
                    success: function (data) {
                        $('.newMsgsPuce').text(data['nbMsgs']);
                    },
                    error: function (xhr, status, error) {
                    }
                });
            });
        </script>
    {% endblock %}

</head>
<body class="bgcolor3">

<div class="loader-backdrop">           <!-- Loader -->
    <div class="loader">
        <div class="ball-1"></div>
        <div class="ball-2"></div>
    </div>
</div>

<header>
    <nav class="navbar navbar-default navbar-fixed-top">         <!-- Navigation Bar -->
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#main-navigation" aria-expanded="false">
                    <span class="sr-only">Toggle Menu</span>
                    <span>Menu</span>
                </button>
                <a class="navbar-brand logoPtf" href=".">
                    {% if template == 'studit'  %}
                        <img src="{{ asset('educomp/images/logo_empty.png') }}" alt="">
                        <img class="logoAbsolute logoGrand" src="{{ asset('educomp/images/logoSTUDIT.png') }}" alt="">
                        <img class="logoAbsolute logoPetit" src="{{ asset('educomp/images/logo_petitSTUDIT.png') }}" alt="">
                        <img class="logoAbsolute logoToutPetit" src="{{ asset('educomp/images/logo_1STUDIT.png') }}" alt="">
                    {% elseif template == "afadec" %}
                        <img src="{{ asset('educomp/images/logo_empty.png') }}" alt="">
                        <img class="logoAbsolute logoGrand" src="{{ asset('educomp/images/logo.png') }}" alt="">
                        <img class="logoAbsolute logoPetit" src="{{ asset('educomp/images/logo_petit.png') }}" alt="">
                        <img class="logoAbsolute logoToutPetit" src="{{ asset('educomp/images/logo_1.png') }}" alt="">
                    {% endif %}
                </a> <!-- Replace with your Logo -->

            </div>

            <div class="collapse navbar-collapse" id="main-navigation">         <!-- Main Menu -->
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="{{ path('homepage') }}">Accueil</a></li>
                    {% if template == 'studit'  %}
                        <li><a href="{{ path('myCourses') }}">Collections</a></li>
                    {% elseif template == 'afadec' %}
                        <li><a href="{{ path('afadec') }}">L’AFADEC</a></li>
                        <li><a href="#">Informations pratiques</a>
                            <ul class="sub-menu">									<!-- Sub Menu -->
                                <li><a href="{{ path('concours') }}">Concours</a></li>
                                <li><a href="{{ path('faq') }}">FAQ</a></li>
                                <li><a href="{{ path('metierEnseignant') }}">Le métier d'enseignant</a></li>
                                <li><a href="{{ path('inscription') }}">Comment s'inscrire ?</a></li>
                                <li><a href="{{ path('cookies') }}">Cookies</a></li>
                            </ul>
                        </li>
                        <li><a href="{{ path('myCourses') }}">Les cours</a></li>
                    {% endif %}

                    {% if app.user %}
                        <li>
                            <a href="#" data-userId="{{ app.user.id }}">{{ app.user.firstname }} {{ app.user.lastname }}</a>
                            <ul class="sub-menu" style="margin-left: -30px;">
                                {% if app.user.hasRole('ROLE_SUPER_ADMIN') %}
                                    <li><a target="_blank" href="{{ path('sonata_admin_dashboard') }}">BackOffice</a></li>
                                    <li><a href="{{ path('emailing') }}">Emailing</a></li>
                                    <li><a href="{{ path('stats') }}">Statistiques</a></li>
                                    <li><a href="{{ path('usersManag') }}">Gestion des utilisateurs</a></li>
                                {% endif %}
                                {% block vueAdmin %}{% endblock %}
                                <li><a href="{{ path('calendrier') }}">Mon calendrier</a></li>
                                <li><a href="{{ path('messagerie') }}">Messagerie</a><span class="newDocsPuce newMsgsPuce"></span></li>
                                <li><a href="{{ path('fos_user_security_logout') }}">Déconnexion</a></li>
                            </ul>
                        </li>
                    {% else %}
                        <li><a class="pull-right btn btn-info account" href="{{ path('fos_user_security_login') }}">Login</a></li>
                    {% endif %}
                </ul>
            </div>
        </div>
    </nav>
</header>



{% block pageHeader %}{% endblock %}
{% block main %}{% endblock %}



<footer class="footer">         <!-- Footer -->
    <div class="footer-pri">            <!-- Primary Footer -->
        <div class="container-fluid">
            <div class="row">
                {% if template == 'studit'  %}
                    <div class="col-sm-4 footer-widget">
                        <h5 class="heading inverse">Informations <span class="color2">pratiques</span></h5>
                        <ul class="quick-links">
                            <li><a target="_blank" href="http://studit.fr/">Studit en ligne</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 footer-widget">
                        <h5 class="heading inverse">Nos <span class="color2">partenaires</span></h5>
                        <ul class="affiliations">
                            </ul>
                    </div>
                    <div class="col-sm-4 footer-widget">            <!-- Footer Widget - Contact Info -->
                        <h5 class="heading inverse">Nous <span class="color2">contacter</span></h5>
                        <!--<a href="" class="email"><i class="fa fa-facebook fa-fw"></i> notre Facebook</a>-->
                        <a href="mailto:contact.contact@studit.fr" class="email"><i class="fa fa-envelope fa-fw"></i> contact@studit.fr</a>
                    </div>
                {% elseif template == 'afadec' %}
                    <div class="col-sm-4 footer-widget">
                        <h5 class="heading inverse">Informations <span class="color2">pratiques</span></h5>
                        <ul class="quick-links">
                            <li><a href="{{ path('concours') }}">Concours</a></li>
                            <li><a href="{{ path('faq') }}">FAQ</a></li>
                            <li><a href="{{ path('metierEnseignant') }}">Le métier d'enseignant</a></li>
                            <li><a href="{{ path('inscription') }}">Comment s'inscrire ?</a></li>
                            <li><a href="{{ path('afadec') }}">L’AFADEC</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 footer-widget">
                        <h5 class="heading inverse">Nos <span class="color2">partenaires</span></h5>
                        <ul class="affiliations">
                            <li><a target="_blank" href="http://enseignement-catholique.fr/"><img src="{{ asset('educomp/images/logo_1.png') }}" class="img-responsive" alt=""></a></li>
                            <li><a target="_blank" href="http://www.udesca.fr/"><img src="{{ asset('educomp/images/logo_2.png') }}" class="img-responsive" alt=""></a></li>
                            <li><a target="_blank" href="http://www.unisfec.fr/"><img src="{{ asset('educomp/images/logo_3.png') }}" class="img-responsive" alt=""></a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4 footer-widget">            <!-- Footer Widget - Contact Info -->
                        <h5 class="heading inverse">Nous <span class="color2">contacter</span></h5>
                        <!--<a href="" class="email"><i class="fa fa-facebook fa-fw"></i> notre Facebook</a>-->
                        <a href="mailto:contact.afadec@gmail.com" class="email"><i class="fa fa-envelope fa-fw"></i> contact.afadec@gmail.com</a>
                    </div>
                {% endif %}
            </div>
        </div>
    </div>
    <div class="footer-sec">            <!-- Secondary Footer -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-6 text-center">
                    {% if template == 'studit'  %}
                        <span class="copyright">&copy; STUDIT 2017</span>
                    {% elseif template == 'afadec' %}
                        <span class="copyright">&copy; AFADEC 2017</span>
                    {% endif %}
                </div>
                <div class="col-sm-6 text-center">
                    <ul class="social">          <!-- Social Media Links -->
                        <!--<li><a href="#"><i class="fa fa-facebook"></i></a></li>-->
                    </ul>
                </div>
            </div>
        </div>
    </div>
</footer>

<div id="back"><i class="fa fa-angle-up"></i></div>     <!-- Back To Top -->
</body>
</html>
